//Fill cube with particles
polyCube -w 3 -h 3 -d 3 -sx 1 -sy 1 -sz 1 -ax 0 1 0 -cuv 4 -ch 1 -n cubeFill;
move -r 0 5 0 ;

evalDeferred "particleFill -rs 10 -maxX 1 -maxY 1 -maxZ 1 -minX 0 -minY 0 -minZ 0 -pd 1";

for($i=0; $i<1000; $i++) 
{
    string $eList[] = `evalDeferred -list`;    
    if( size($eList) == 0 ) break;
}

